generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id             String    @id @default(cuid()) @map("_id")
  name           String?
  email          String?   @unique
  hashedPassword String
  userRole       UserRole  @default(USER)
  createdAt      DateTime  @default(now()) @map("created_at")
  updatedAt      DateTime  @updatedAt @map("updated_at")
  accounts       Account[]
  nurseries      Nursery?
}

model Account {
  id                String  @id @default(cuid()) @map("_id")
  userId            String
  type              String
  provider          String
  refresh_token     String?
  access_token      String?
  expires_at        Int?
  token_type        String?
  scope             String?
  id_token          String?
  providerAccountId String
  session_state     String?
  user              User    @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@unique([provider, providerAccountId])
  @@index([userId], map: "Account_userId_fkey")
}

model Nursery {
  id        String           @id @default(cuid()) @map("_id")
  name      String
  address   String?
  phone     String?
  email     String?
  website   String?
  logoSrc   String?
  createdAt DateTime         @default(now()) @map("created_at")
  updatedAt DateTime         @updatedAt @map("updated_at")
  ownerId   String           @unique @map("owner_id")
  owner     User             @relation(fields: [ownerId], references: [id], onDelete: Cascade)
  species   NurserySpecies[]
}

model NurserySpecies {
  id        String  @id @default(cuid()) @map("_id")
  nurseryId String  @map("nursery_id")
  speciesId String  @map("species_id")
  nursery   Nursery @relation(fields: [nurseryId], references: [id], onDelete: Cascade)

  @@unique([nurseryId, speciesId], map: "NurserySpecies_nurseryId_speciesId_key")
  @@index([nurseryId], map: "NurserySpecies_nursery_id_fkey")
  @@index([speciesId], map: "NurserySpecies_species_id_fkey")
}

model Species {
  id                String         @id @default(cuid())
  name              String         @unique
  createdAt         DateTime       @default(now()) @map("created_at")
  updatedAt         DateTime       @updatedAt @map("updated_at")
  availables_status Boolean        @default(true) @map("available_status")
  arboriculture     Arboriculture?
  ecology           Ecology?
  ethnobotany       Ethnobotany?
  flower            Flower?
  images            Images?
  leaf              Leaf?
  root              Root?
  seeds             Seeds?
  stalk             Stalk?
  taxonomy          Taxonomy?
}

model Taxonomy {
  id           String  @id @default(cuid())
  family       String? @default("No definido")
  genus        String? @default("No definido")
  tSpecies     String? @default("No definido")
  subspecies   String? @default("No definido")
  variety      String? @default("No definido")
  author       String? @default("No definido")
  synonyms     String? @default("No definido")
  etymology    String? @default("No definido") @db.VarChar(500)
  common_names String? @default("No definido")
  growth_habit String? @default("No definido")
  bibliography String? @default("No definido") @db.VarChar(500)
  speciesId    String  @unique @map("species_id")
  species      Species @relation(fields: [speciesId], references: [id], onDelete: Cascade)

  @@index([speciesId], map: "Taxonomy_species_id_fkey")
}

model Stalk {
  id              String  @id @default(cuid())
  bark_attributes String? @default("No definido")
  barkColor       String? @default("No definido")
  photoBark       String? @default("No definido")
  speciesId       String  @unique @map("species_id")
  species         Species @relation(fields: [speciesId], references: [id], onDelete: Cascade)

  @@index([speciesId], map: "Stalk_species_id_fkey")
}

model Root {
  id                String  @id @default(cuid())
  reproduction_form String? @default("No definido")
  root_attributes   String? @default("No definido")
  rooting_type      String? @default("No definido")
  speciesId         String  @unique @map("species_id")
  species           Species @relation(fields: [speciesId], references: [id], onDelete: Cascade)

  @@index([speciesId], map: "Root_species_id_fkey")
}

model Flower {
  id                 String  @id @default(cuid())
  floral_attributes  String? @default("No definido")
  flower_color       String? @default("No definido")
  flower_arrangement String? @default("No definido")
  flowering_season   String? @default("No definido")
  flowering_months   String? @default("No definido")
  speciesId          String  @unique @map("species_id")
  pollination_system String? @default("No definido")
  species            Species @relation(fields: [speciesId], references: [id], onDelete: Cascade)

  @@index([speciesId], map: "Flower_species_id_fkey")
}

model Leaf {
  id                String  @id @default(cuid())
  leaf_attributes   String? @default("No definido")
  leaf_persistence  String? @default("No definido")
  stemLeaf_position String? @default("No definido")
  leaf_composition  String? @default("No definido")
  speciesId         String  @unique @map("species_id")
  species           Species @relation(fields: [speciesId], references: [id], onDelete: Cascade)

  @@index([speciesId], map: "Leaf_species_id_fkey")
}

model Seeds {
  id               String  @id @default(cuid())
  fruitType        String? @default("No definido")
  dispersal_system String? @default("No definido")
  fruit_attributes String? @default("No definido")
  seed_attributes  String? @default("No definido")
  fruiting_months  String? @default("No definido")
  speciesId        String  @unique @map("species_id")
  species          Species @relation(fields: [speciesId], references: [id], onDelete: Cascade)

  @@index([speciesId], map: "Seeds_species_id_fkey")
}

model Ecology {
  id           String  @id @default(cuid())
  use_category String? @default("No definido")
  use_detail   String? @default("No definido")
  speciesId    String  @unique @map("species_id")
  species      Species @relation(fields: [speciesId], references: [id], onDelete: Cascade)

  @@index([speciesId], map: "Ecology_species_id_fkey")
}

model Ethnobotany {
  id                  String  @id @default(cuid())
  altitudinal_range   String? @default("No definido")
  geo_distribution    String? @default("No definido")
  origin              String? @default("No definido")
  conservation_status String? @default("No definido")
  speciesId           String  @unique @map("species_id")
  species             Species @relation(fields: [speciesId], references: [id], onDelete: Cascade)

  @@index([speciesId], map: "Ethnobotany_species_id_fkey")
}

model Arboriculture {
  id                 String  @id @default(cuid())
  public_spaceUse    String? @default("No definido")
  flower_limitations String? @default("No definido")
  fruit_limitations  String? @default("No definido")
  longevity          String? @default("No definido")
  pests_diseases     String? @default("No definido")
  fauna_attraction   String? @default("No definido")
  associated_fauna   String? @default("No definido")
  light_requirements String? @default("No definido")
  growth_rate        String? @default("No definido")
  maximum_height     String? @default("No definido")
  crown_width        String? @default("No definido")
  crown_shape        String? @default("No definido")
  DAP                String? @default("No definido")
  foliage_density    String? @default("No definido")
  soil_type          String? @default("No definido")
  humidity_zone      String? @default("No definido")
  speciesId          String  @unique @map("species_id")
  species            Species @relation(fields: [speciesId], references: [id], onDelete: Cascade)

  @@index([speciesId], map: "Arboriculture_species_id_fkey")
}

model Images {
  id               String  @id @default(cuid())
  presentation_url String? @default("No definido")
  fruit_url        String? @default("No definido")
  leaf_url         String? @default("No definido")
  flower_url       String? @default("No definido")
  detailFlower_url String? @default("No definido")
  bark_url         String? @default("No definido")
  seed_url         String? @default("No definido")
  speciesId        String  @unique @map("species_id")
  species          Species @relation(fields: [speciesId], references: [id], onDelete: Cascade)

  @@index([speciesId], map: "Image_species_id_fkey")
}

enum UserRole {
  SPECIES_ADMIN
  NURSERY_ADMIN
  ADMIN
  USER
}
